                            <%= link_to 'phat tu dinh quan', 'javascript:void(0)', title: 'phat tu dinh quan', rel: 'tag' %>
                            <%= link_to 'tin tuc phat giao dinh quan', 'javascript:void(0)', title: 'tin tuc phat giao dinh quan', rel: 'tag' %>
                            <%= link_to 'phat su', 'javascript:void(0)', title: 'phat su', rel: 'tag' %>
                            <%= link_to 'gdpt dinh quan', 'javascript:void(0)', title: 'gdpt dinh quan', rel: 'tag' %>
                            <%= link_to 'phap am phat giao', 'javascript:void(0)', title: 'phap am phat giao', rel: 'tag' %>
