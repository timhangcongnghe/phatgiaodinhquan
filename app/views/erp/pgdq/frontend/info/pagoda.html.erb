                  <div class="post-bottom-meta post-bottom-tags post-tags-modern">
                        <div class="post-bottom-meta-title"><span class="tie-icon-tags" aria-hidden="true"></span> Nhãn bài viết</div>
                        <span class="tagcloud">
                            <%= link_to 'phat giao dong nai', 'javascript:void(0)', title: 'phat giao dong nai', rel: 'tag' %>
                            <%= link_to 'phat giao dinh quan', 'javascript:void(0)', title: 'phat giao dinh quan', rel: 'tag' %>
